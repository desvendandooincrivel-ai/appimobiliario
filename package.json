{
  "name": "jobh-imoveis-manager",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "homepage": "./",
  "description": "Gerenciador de Imóveis Jobh",
  "author": "Jobh",
  "repository": {
    "type": "git",
    "url": "https://github.com/desvendandooincrivel-ai/appimobiliario.git"
  },
  "dependencies": {
    "electron-log": "^5.4.3",
    "electron-updater": "^6.7.3",
    "lucide-react": "latest",
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^8.0.0",
    "cross-env": "^7.0.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.0",
    "typescript": "^5.0.0",
    "vite": "^7.3.0",
    "wait-on": "^7.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently \"cross-env BROWSER=none npm run dev\" \"electron .\"",
    "electron:build": "npm run build && electron-builder -w"
  },
  "build": {
    "appId": "com.jobhimoveis.manager",
    "productName": "Jobh Imóveis Manager",
    "files": [
      "build/**/*",
      "main.js",
      "preload.js"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": "portable"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "desvendandooincrivel-ai",
        "repo": "appimobiliario"
      }
    ]
  }
}